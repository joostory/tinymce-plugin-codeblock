{
  "name": "tinymce-plugin-codeblock",
  "version": "2.0.1",
  "description": "CodeBlock TinyMCE plugin",
  "main": "dist/codeblock/plugin.js",
  "module": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist lib",
    "start": "webpack-dev-server --progress --inline --mode development --content-base static",
    "lint": "eslint 'src/**/*.js'",
    "build:plugin": "webpack --mode production",
    "build:es": "babel src --out-dir lib && cp -R src/styles lib",
    "build": "npm run build:plugin && npm run build:es",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "codemirror": "^5.48.4",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "highlightjs": "^9.12.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "tinymce": "^5.0.14",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "lodash": "^4.17.15"
  },
  "peerDependencies": {
    "tinymce": ">= 4.0",
    "codemirror": ">= 5.0",
    "highlightjs": ">= 9.0",
    "css-loader": ">= 1.0"
  },
  "license": "MIT"
}
