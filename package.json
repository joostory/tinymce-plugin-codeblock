{
  "name": "tinymce-plugin-codeblock",
  "version": "2.3.0",
  "description": "CodeBlock TinyMCE plugin",
  "main": "dist/codeblock/plugin.js",
  "module": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist lib",
    "start": "webpack-dev-server --progress --inline --mode development --content-base static",
    "lint": "eslint 'src/**/*.js'",
    "build:plugin": "webpack --mode production",
    "build:es": "babel src --out-dir lib && cp -R src/styles lib",
    "build": "npm run build:plugin && npm run build:es",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "author": "",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "codemirror": "^5.59.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "highlight.js": "^10.5.0",
    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^2.0.0",
    "tinymce": "^5.6.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "lodash": "^4.17.20"
  },
  "peerDependencies": {
    "tinymce": ">= 5.0",
    "codemirror": ">= 5.0",
    "highlight.js": ">= 9.0",
    "css-loader": ">= 1.0"
  },
  "license": "MIT"
}
