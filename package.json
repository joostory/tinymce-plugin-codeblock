{
  "name": "tinymce-plugin-codeblock",
  "version": "2.3.1",
  "description": "CodeBlock TinyMCE plugin",
  "main": "dist/codeblock/plugin.js",
  "module": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist lib",
    "start": "node server.js",
    "lint": "eslint 'src/**/*.js'",
    "build:plugin": "webpack --mode production",
    "build:es": "babel src --out-dir lib && cp -R src/styles lib",
    "build": "npm run build:plugin && npm run build:es",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "author": "",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "codemirror": "^5.59.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.5",
    "highlight.js": "^11.5.1",
    "html-webpack-plugin": "^5.3.1",
    "style-loader": "^3.3.1",
    "tinymce": "^5.6.2",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-middleware": "^5.0.0"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "lodash": "^4.17.20"
  },
  "peerDependencies": {
    "codemirror": ">= 5.0",
    "css-loader": ">= 1.0",
    "highlight.js": ">= 9.0",
    "tinymce": ">= 5.0"
  },
  "license": "MIT"
}
